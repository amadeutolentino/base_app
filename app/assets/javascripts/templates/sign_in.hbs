<div class="session_form">
	{{#if loginError}}
		<div class="alert alert-error">
		  <button type="button" class="close" data-dismiss="alert" {{action "dismissError"}}>&times;</button>
		  {{loginResponse}}}
		</div>
	{{/if}}
	<form>
		<fieldset>
			<legend>Log In</legend>
	    <label>Email</label>
	    {{view Ember.TextField type="email" valueBinding="email"}}
	    <label>Password</label>
	    {{view Ember.TextField type="password" valueBinding="password"}}
			<br>
	    <button {{action "signIn"}} class="btn">Log In</button>
		</fieldset>
	</form>
</div>